{
  "openapi": "3.0.0",
  "paths": {
    "/": {
      "get": {
        "operationId": "AppController_getHello",
        "parameters": [],
        "responses": {
          "200": {
            "description": "アプリケーションの状態を返します",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        },
        "summary": "アプリケーションのヘルスチェック",
        "tags": [
          "app"
        ]
      }
    },
    "/demo/quiz": {
      "get": {
        "description": "デモ用のクイズデータを返します",
        "operationId": "DemoController_getQuiz",
        "parameters": [],
        "responses": {
          "200": {
            "description": "デモクイズデータ",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        },
        "summary": "デモクイズの取得",
        "tags": [
          "demo"
        ]
      }
    },
    "/quiz/skill": {
      "get": {
        "description": "ランダムなサーヴァントのスキル情報を含むクイズデータを返します",
        "operationId": "QuizController_getSkillQuiz",
        "parameters": [],
        "responses": {
          "200": {
            "description": "スキルクイズデータ",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ServantDetailGetResponseDto"
                }
              }
            }
          }
        },
        "summary": "スキルクイズの取得",
        "tags": [
          "quiz"
        ]
      }
    },
    "/servants/options": {
      "get": {
        "description": "クイズで使用するサーヴァントの選択肢データを取得します。各サーヴァントのID、名前、オリジナル名が含まれます。",
        "operationId": "ServantsController_getServantOptions",
        "parameters": [],
        "responses": {
          "200": {
            "description": "サーヴァント選択肢一覧の取得に成功しました",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ServantsOptionsGetResponseDto"
                }
              }
            }
          },
          "500": {
            "description": "サーバー内部エラー"
          }
        },
        "summary": "サーヴァント選択肢一覧の取得",
        "tags": [
          "servants"
        ]
      }
    }
  },
  "info": {
    "title": "FGO Servant Quiz API",
    "description": "Fate/Grand Order Servant Quiz APIのドキュメント",
    "version": "1.0",
    "contact": {}
  },
  "tags": [
    {
      "name": "quiz",
      "description": "クイズ関連のエンドポイント"
    },
    {
      "name": "demo",
      "description": "デモ用のエンドポイント"
    }
  ],
  "servers": [],
  "components": {
    "schemas": {
      "NoblePhantasm": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number",
            "description": "ノーブルファンタズムID",
            "example": 101
          },
          "num": {
            "type": "number",
            "description": "ノーブルファンタズム番号",
            "example": 1
          },
          "card": {
            "type": "string",
            "description": "カードタイプ",
            "example": "buster"
          },
          "name": {
            "type": "string",
            "description": "ノーブルファンタズム名",
            "example": "約束された勝利の剣"
          },
          "originalName": {
            "type": "string",
            "description": "オリジナル名",
            "example": "Excalibur"
          },
          "ruby": {
            "type": "string",
            "description": "ルビ",
            "example": "エクスカリバー"
          },
          "icon": {
            "type": "string",
            "description": "アイコンURL"
          },
          "rank": {
            "type": "string",
            "description": "ランク",
            "example": "A++"
          },
          "type": {
            "type": "string",
            "description": "タイプ",
            "example": "対城宝具"
          },
          "effectFlags": {
            "description": "効果フラグ",
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "detail": {
            "type": "string",
            "description": "詳細説明"
          },
          "unmodifiedDetail": {
            "type": "string",
            "description": "未修正の詳細説明"
          }
        },
        "required": [
          "id",
          "num",
          "card",
          "name",
          "originalName",
          "ruby",
          "icon",
          "rank",
          "type",
          "effectFlags",
          "detail",
          "unmodifiedDetail"
        ]
      },
      "Skill": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number",
            "description": "スキルID",
            "example": 501
          },
          "num": {
            "type": "number",
            "description": "スキル番号",
            "example": 1
          },
          "name": {
            "type": "string",
            "description": "スキル名",
            "example": "カリスマ"
          },
          "originalName": {
            "type": "string",
            "description": "オリジナル名",
            "example": "Charisma"
          },
          "ruby": {
            "type": "string",
            "description": "ルビ"
          },
          "detail": {
            "type": "string",
            "description": "スキル詳細説明"
          },
          "unmodifiedDetail": {
            "type": "string",
            "description": "未修正のスキル詳細説明"
          },
          "type": {
            "type": "string",
            "description": "スキルタイプ"
          },
          "priority": {
            "type": "number",
            "description": "優先度",
            "example": 0
          },
          "icon": {
            "type": "string",
            "description": "アイコンURL"
          }
        },
        "required": [
          "id",
          "num",
          "name",
          "originalName",
          "ruby",
          "detail",
          "unmodifiedDetail",
          "type",
          "priority",
          "icon"
        ]
      },
      "ServantDetailGetResponseDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number",
            "description": "サーヴァントID",
            "example": 1
          },
          "collectionNo": {
            "type": "number",
            "description": "コレクション番号",
            "example": 1
          },
          "name": {
            "type": "string",
            "description": "サーヴァント名",
            "example": "アルトリア・ペンドラゴン"
          },
          "originalName": {
            "type": "string",
            "description": "オリジナル名",
            "example": "Artoria Pendragon"
          },
          "ruby": {
            "type": "string",
            "description": "ルビ"
          },
          "classId": {
            "type": "number",
            "description": "クラスID",
            "example": 1
          },
          "rarity": {
            "type": "number",
            "description": "レア度",
            "example": 5
          },
          "noblePhantasms": {
            "description": "ノーブルファンタズム一覧",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/NoblePhantasm"
            }
          },
          "skills": {
            "description": "スキル一覧",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Skill"
            }
          }
        },
        "required": [
          "id",
          "collectionNo",
          "name",
          "originalName",
          "ruby",
          "classId",
          "rarity",
          "noblePhantasms",
          "skills"
        ]
      },
      "ServantsOptions": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number",
            "description": "サーヴァントID",
            "example": 1
          },
          "name": {
            "type": "string",
            "description": "サーヴァント名",
            "example": "アルトリア・ペンドラゴン"
          },
          "originalOverwriteName": {
            "type": "string",
            "description": "オリジナル名",
            "example": "Artoria Pendragon"
          },
          "classId": {
            "type": "number",
            "description": "クラスID",
            "example": 1
          }
        },
        "required": [
          "id",
          "name",
          "originalOverwriteName",
          "classId"
        ]
      },
      "ServantsOptionsGetResponseDto": {
        "type": "object",
        "properties": {
          "options": {
            "description": "サーヴァント選択肢の配列",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ServantsOptions"
            }
          }
        },
        "required": [
          "options"
        ]
      }
    }
  }
}